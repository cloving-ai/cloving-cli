{
  "name": "cloving",
  "version": "0.3.18",
  "packageManager": "yarn@1.22.22",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && cp package.json dist/package.json && cp src/utils/prompts/*.md dist/utils/prompts/",
    "start": "node dist/index.js",
    "watch": "tsc -w",
    "check": "bash -c 'npx tsc --noEmit --skipLibCheck'",
    "test": "bash -c jest",
    "prepare": "husky",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "bin": "./dist/index.js",
  "files": [
    "dist",
    "!**/test/**"
  ],
  "keywords": [],
  "author": "Lucas Carlson",
  "license": "MIT",
  "description": "",
  "lint-staged": {
    "*.ts": [
      "yarn run check",
      "prettier --write"
    ]
  },
  "lint-prepush": {
    "base": "main",
    "tasks": {
      "*.ts": [
        "yarn run check",
        "yarn run test",
        "yarn run format"
      ]
    }
  },
  "dependencies": {
    "@inquirer/prompts": "^7.3.2",
    "axios": "^1.7.9",
    "cli-highlight": "^2.1.11",
    "colors": "^1.4.0",
    "commander": "^13.1.0",
    "copy-paste": "^1.5.3",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-config-raine": "^0.5.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-promise": "^7.2.1",
    "express": "^4.21.2",
    "ignore": "^7.0.3",
    "ini": "^5.0.0",
    "isbinaryfile": "^5.0.4",
    "nock": "^14.0.1",
    "vite": "^6.1.1",
    "vite-node": "^3.0.6"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/copy-paste": "^1.1.33",
    "@types/express": "^5.0.0",
    "@types/ini": "^4.1.1",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-prepush": "^3.0.2",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  },
  "resolutions": {
    "eslint": "8.57.0",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-promise": "6.6.0"
  }
}
